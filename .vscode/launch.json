{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "LLDB Debugger",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/simulation",
      // "program": "${workspaceFolder}/Tests/Test 1/main-g",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
